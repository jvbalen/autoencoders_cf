# 1-layer WAE with NLL loss, bias and normalization

run_experiment.Recommender = @TFRecommender

preprocess.tfidf = True

TFRecommender.Model = @WAE
TFRecommender.n_layers = 1
TFRecommender.weights_path = '/Users/jan/experiments/wae/logistic_sparse.npz'

WAE.loss = "bce"
WAE.randomize_inits = False
WAE.use_biases = True
WAE.normalize_inputs = True
WAE.keep_prob = 1.0
WAE.lam = 0.0
WAE.lr = 3e-4
